        <activity android:name="com.guitargigkok.activity.ChordingActivity" >
        </activity>
        <activity android:name="com.guitargigkok.activity.StrumActivity" >
        </activity>
        <activity android:name="com.guitargigkok.activity.SettingActivity" >
        </activity>
        <activity android:name="com.guitargigkok.activity.AcousActivity" >
        </activity>
        <activity android:name="com.guitargigkok.activity.ExamChordActivity" >
        </activity>
